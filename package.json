{
  "name": "events-app",
  "version": "1.0.1",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "build:web": "expo export --platform web --output-dir web-build",
    "build:web:clear": "expo export --platform web --output-dir web-build --clear",
    "build:web:prod": "powershell -ExecutionPolicy Bypass -File ./scripts/build-web-release.ps1 -Channel prod",
    "build:web:demo": "powershell -ExecutionPolicy Bypass -File ./scripts/build-web-release.ps1 -Channel demo",
    "build:play:aab:prod": "powershell -ExecutionPolicy Bypass -File ./scripts/build-play-aab.ps1 -Channel prod",
    "build:play:aab:demo": "powershell -ExecutionPolicy Bypass -File ./scripts/build-play-aab.ps1 -Channel demo",
    "stripe:live:secrets": "powershell -ExecutionPolicy Bypass -File ./scripts/set-stripe-live-secrets.ps1",
    "deploy:netlify": "powershell -ExecutionPolicy Bypass -File ./scripts/deploy-netlify-prod.ps1",
    "deploy:netlify:demo": "powershell -ExecutionPolicy Bypass -File ./scripts/deploy-netlify-prod.ps1 -Channel demo",
    "deploy:netlify:skip-build": "powershell -ExecutionPolicy Bypass -File ./scripts/deploy-netlify-prod.ps1 -SkipBuild",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@expo/metro-runtime": "~3.2.3",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@supabase/supabase-js": "^2.96.0",
    "expo": "^51.0.39",
    "expo-clipboard": "~8.0.8",
    "expo-document-picker": "~14.0.8",
    "expo-file-system": "^19.0.21",
    "expo-linear-gradient": "^15.0.8",
    "expo-sharing": "^14.0.8",
    "expo-status-bar": "~3.0.9",
    "pdf-lib": "^1.17.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.74.5",
    "react-native-google-mobile-ads": "^16.0.3",
    "react-native-qrcode-svg": "^6.3.21",
    "react-native-web": "^0.21.0"
  },
  "devDependencies": {
    "@react-native/typescript-config": "^0.84.0",
    "@types/react": "~18.2.6",
    "supabase": "^2.76.12",
    "typescript": "~5.9.2"
  },
  "private": true
}
